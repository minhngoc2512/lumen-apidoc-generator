{
    "name": "minhngoc/lumen-apidoc-generator-minhngoc",
    "license": "MIT",
    "description": "Generate beautiful API documentation using Dingo router from your Lumen application",
    "keywords": [
        "API",
        "Documentation",
        "Lumen",
        "Laravel"
    ],
    "homepage": "http://github.com/minhngoc2512/lumen-apidoc-generator",
    "authors": [
        {
            "name": "Tung"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "fzaninotto/faker": "~1.0",
        "laravel/framework": "~5.4",
        "ramsey/uuid": "^3.0",
        "mnapoli/front-yaml": "^1.5",
        "mnapoli/silly": "~1.0",
        "illuminate/view": "5.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~4.0 || ~5.0",
        "dingo/api": "2.0.0-alpha1",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "files": [
            "src/MinhNgoc/ApiDoc/Documentarian/helpers.php"
        ],
        "psr-0": {
            "MinhNgoc\\ApiDoc": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MinhNgoc\\ApiDoc\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MinhNgoc\\ApiDoc\\ApiDocGeneratorServiceProvider"
            ]
       }
    }
}
